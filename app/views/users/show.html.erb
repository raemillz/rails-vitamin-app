<header class="masthead text-center text-white">
  <p id="notice"><%= notice %></p>

  <% if @message %>
    <div class="center alert">
      <p><%= @message %></p>
    </div>
  <% end %>
  <div class="bg-circle-1 bg-circle"></div>
  <div class="bg-circle-2 bg-circle"></div>
  <div class="bg-circle-3 bg-circle"></div>
  <div class="bg-circle-4 bg-circle"></div>
</header>

<section>

  <div class="container text-center">

    <div class="center">
      <h1><%= @user.name %>'s Vitamin Packs</h1>
      <section>
        <div class="info">
          <strong>
            <% @user.vitamin_packs.each do |pack| %>
              <h3><li><%= link_to pack.name, user_vitamin_pack_path(@user, pack) %></li></h3>
            <% end %>
          </strong>
        </div>
      </section>
    </div>
  </div>

  <div class="container text-center">
    <sub><%= link_to 'Create New Vitamin Pack', new_user_vitamin_pack_path(@user) %></sub> </br>
    <sub><%= link_to 'Edit Profile', edit_user_path(@user) %></sub>
  </div>
  
</section>
